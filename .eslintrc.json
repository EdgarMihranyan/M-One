{
     "env": {
          "browser": true,
          "es2021": true
     },
     "extends": [
          "airbnb-base"
     ],
     "parserOptions": {
          "ecmaVersion": "latest",
          "sourceType": "module"
     },
     "rules": {
          "linebreak-style": "off",
          "indent": [
               "error",
               5
          ],
          "no-console": "off",
          "import/extensions": "off",
          "no-underscore-dangle": "off",
          "import/prefer-default-export": "off",
          "no-plusplus": "off"
     }
}